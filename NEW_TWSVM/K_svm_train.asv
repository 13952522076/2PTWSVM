function model=K_svm_train(label, Inst, C,v) model = svm(label,Inst,C,v);%==========================================================================function model=svm(d,A,C,v);for i = 1:vCtest = []; dtest = [];Ctrain = []; dtrain = [];Ctest = A((indx(i)+1:indx(i+1)),:);dtest = d(indx(i)+1:indx(i+1));Ctrain = A(1:indx(i),:);Ctrain = [Ctrain;A(indx(i+1)+1:sm,:)];dtrain = [d(1:indx(i));d(indx(i+1)+1:sm,:)];[w1,w2,bias1,bias2]=svc(Ctrain,dtrain,C);[err ]= svcerror(w1,w2,bias1,bias2,Ctest,dtest);VErrList(i)=1-err/length(Ctest(:,1));[err ]= svcerror(w1,w2,bias1,bias2,Ctrain,dtrain); TErrList(i)=1-err/length(Ctrain(:,1));                  end           TErr=mean(TErrList);        VErr=mean(VErrList);                      if(exist('TErr', 'var') && exist('VErr', 'var'))    model.Err.Training=TErr;    model.Err.Validation=VErr;else    model.Err.Training=NaN;    model.Err.Validation=NaN;end%==========================================================================